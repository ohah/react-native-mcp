{
  "name": "@ohah/react-native-mcp-server",
  "version": "0.1.0-rc.8",
  "description": "MCP server for React Native app automation and monitoring",
  "keywords": [
    "automation",
    "mcp",
    "model-context-protocol",
    "monitoring",
    "react-native"
  ],
  "homepage": "https://github.com/ohah/react-native-mcp#readme",
  "bugs": {
    "url": "https://github.com/ohah/react-native-mcp/issues"
  },
  "license": "MIT",
  "author": "ohah",
  "repository": {
    "type": "git",
    "url": "https://github.com/ohah/react-native-mcp.git",
    "directory": "packages/react-native-mcp-server"
  },
  "bin": {
    "react-native-mcp-server": "dist/index.js"
  },
  "files": [
    "dist",
    "runtime.js",
    "babel-preset.cjs",
    "babel-plugin-app-registry.cjs",
    "babel-plugin-inject-testid.cjs",
    "scripts/doctor.mjs"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./symbolicate": {
      "types": "./dist/symbolicate.d.ts",
      "import": "./dist/symbolicate.js",
      "require": "./dist/symbolicate.js"
    },
    "./runtime": "./runtime.js",
    "./runtime.js": "./runtime.js",
    "./babel-preset": "./babel-preset.cjs",
    "./babel-plugin-app-registry": "./babel-plugin-app-registry.cjs",
    "./babel-plugin-inject-testid": "./babel-plugin-inject-testid.cjs",
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "import": "./dist/test/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bunx tsdown && node scripts/chmod-dist.mjs",
    "prepublishOnly": "bun run build",
    "dev": "bun run --watch src/index.ts",
    "mcp:run": "bun dist/index.js",
    "mcp": "bun run build && bun dist/index.js",
    "test": "bun test src/",
    "doctor": "node scripts/doctor.mjs"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.0",
    "fast-xml-parser": "^5.3.6",
    "pixelmatch": "^7.1.0",
    "sharp": "^0.34.5",
    "source-map": "^0.7.6",
    "ws": "^8.18.0",
    "yaml": "^2.8.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.26.0",
    "@babel/parser": "^7.26.0",
    "@babel/traverse": "^7.26.0",
    "@babel/types": "^7.26.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.28.0",
    "@types/bun": "latest",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.0",
    "metro": "^0.83.3",
    "tsdown": "0.20.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react-native": ">=0.74.0"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=24"
  }
}
