{
  "name": "@ohah/react-native-mcp-server",
  "version": "0.1.0001-rc.1",
  "description": "MCP server for React Native app automation and monitoring",
  "keywords": [
    "automation",
    "mcp",
    "model-context-protocol",
    "monitoring",
    "react-native"
  ],
  "homepage": "https://github.com/ohah/react-native-mcp#readme",
  "bugs": {
    "url": "https://github.com/ohah/react-native-mcp/issues"
  },
  "license": "MIT",
  "author": "ohah",
  "repository": {
    "type": "git",
    "url": "https://github.com/ohah/react-native-mcp.git",
    "directory": "packages/react-native-mcp-server"
  },
  "bin": {
    "react-native-mcp-server": "dist/index.js"
  },
  "files": [
    "dist",
    "runtime.js",
    "cdp-interceptor.cjs",
    "babel-preset.cjs",
    "babel-plugin-app-registry.cjs",
    "babel-plugin-inject-testid.cjs"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./cdp-interceptor": "./cdp-interceptor.cjs",
    "./runtime": "./runtime.js",
    "./runtime.js": "./runtime.js",
    "./babel-preset": "./babel-preset.cjs",
    "./babel-plugin-app-registry": "./babel-plugin-app-registry.cjs",
    "./babel-plugin-inject-testid": "./babel-plugin-inject-testid.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bunup && bun build src/index.ts --outdir=dist --target=node --external=sharp && bun build src/transformer-entry.ts --outdir=dist --target=node && bun build src/babel-plugin-app-registry.ts --outfile=dist/babel-plugin-app-registry.cjs --target=node --format=cjs && bun build src/babel-plugin-inject-testid.ts --outfile=dist/babel-plugin-inject-testid.cjs --target=node --format=cjs && bun scripts/chmod-dist.mjs",
    "prepublishOnly": "bun run build",
    "dev": "bun run --watch src/index.ts",
    "mcp:run": "bun dist/index.js",
    "mcp": "bun run build && bun dist/index.js",
    "test": "bun test src/"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.0",
    "fast-xml-parser": "^5.3.6",
    "sharp": "^0.34.5",
    "ws": "^8.18.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.26.0",
    "@babel/parser": "^7.26.0",
    "@babel/traverse": "^7.26.0",
    "@babel/types": "^7.26.0",
    "@ohah/react-native-mcp-server": "workspace:*",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.28.0",
    "@types/bun": "latest",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.0",
    "bunup": "^0.16.22",
    "metro": "^0.83.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=24"
  }
}
